@model RevenueViewModel

@{
    ViewData["Title"] = "Index";
}

<p>
    <a asp-action="Create">Revenue</a>
</p>
<form class="form-inline" asp-action="Revenue">
    @foreach (var item in Model.RevenuTypesList)
    {
        <div class="form-group mb-2">
            <label asp-for="@item.Name" class="control-label">@item.Name</label>
        </div>
        <div class="form-group mx-sm-3 mb-2">
            <input for="InBirr" class="form-control" id="" placeholder="Birr">
        </div>
    }
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>